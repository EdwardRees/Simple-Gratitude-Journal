{
  "expo": {
    "name": "Simple Gratitude Journal",
    "slug": "Gratitude-Journal",
    "description": "A simple gratitude journal created to help others become more mindful about the life they life.",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "githubUrl": "https://github.com/EdwardRees/Simple-Gratitude-Journal",
    "userInterfaceStyle": "automatic",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "package": "info.edwardrees.simplegratitudejournal",
      "useNextNotificationsApi": true
    }
  }
}
